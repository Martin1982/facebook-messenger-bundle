    "autoload": {
        "psr-4": { "PouleR\\FacebookMessengerBundle\\": "" }
    },
    "require": {
        "php": ">=7.0",
        "symfony/serializer": "^3.0|^4.0",
        "symfony/property-access": "^3.0|4.0",
        "symfony/framework-bundle": "^3.0|^4.0",
